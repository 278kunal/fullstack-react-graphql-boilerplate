    "start": "ts-node index.ts",
    "watch": "tsnd --respawn --transpileOnly --no-notify --ignore-watch node_modules index.ts",
    "dev-debug": "NODE_ENV=development node --inspect -r ts-node/register server/index.ts"
  },
